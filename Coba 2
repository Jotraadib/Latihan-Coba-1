  display1.clearDisplay();
  // Display bitmap
  display1.drawBitmap(0, 0, i0_Black, 128, 64, WHITE);
  display1.display();
  delay(2000);
}
void gambarclear() {
  display1.clearDisplay();
  // Display bitmap
  display1.drawBitmap(0, 0, clear, 128, 64, WHITE);
  display1.display();
}
void gambarpassword() {
  display1.clearDisplay();
  // Display bitmap
  display1.drawBitmap(0, 0, password, 128, 64, WHITE);
  display1.display();
}
void gambarwrong() {
  display1.clearDisplay();
  // Display bitmap
  display1.drawBitmap(0, 0, Wrong, 128, 64, WHITE);
  display1.display();
}
void gambarbenar() {
  display1.clearDisplay();
  // Display bitmap
  display1.drawBitmap(0, 0, Correct, 128, 64, WHITE);
  display1.display();
}
void gambarcolor() {
  display1.clearDisplay();
  // Display bitmap
  display1.drawBitmap(0, 0, icolor, 128, 64, WHITE);
  display1.display();
}
void setup() {
  // put your setup code here, to run once:
  pinMode(51, OUTPUT);
  pinMode(50, OUTPUT);
  pinMode(52, OUTPUT);
  pinMode(BUZZER, OUTPUT);

  display.setBrightness(0x0f);  //7seg

  // initialize the OLED object
  if (!display1.begin(SSD1306_SWITCHCAPVCC, SCREEN_ADDRESS)) {
    Serial.println(F("SSD1306 allocation failed"));
    for (;;)
      ;  // Don't proceed, loop forever
  }
  //rgb
  Serial.begin(9600);
  if (tcs.begin()) {
  } else {
    Serial.println("No TCS34725 found ... check your connections");
    while (1)
      ;
  }
  for (int i = 0; i < 256; i++) {
    float x = i;
    x /= 255;
    x = pow(x, 2.5);
    x *= 255;

    if (commonAnode) {
      gammatable[i] = 255 - x;
    } else {
      gammatable[i] = x;
    }
  }

  //accelStepper
  stepper.setMaxSpeed(1000.0);
  stepper.setAcceleration(50.0);
  stepper.setSpeed(200);
  //accelStepper
}
void loop() {
  // put your main code here, to run repeatedly:
  while (menu == 0) {
    kelip();
    gambarhello();
    myStepper.setSpeed(10);
    myStepper.step(-stepsPerRevolution);
    // step one revolution in the other direction:
    myStepper.setSpeed(10);
    myStepper.step(stepsPerRevolution);
    delay(100);
    display.showNumberDec(0, true);
    delay(300);
    menu = 1;
  }
  while (menu == 1) {
    gambarpassword();
    led(0, 1, 0);
    display.showNumberDec(0, true);
    menu = 2;
  }
  while (menu == 2) {
    gambarpassword();
    char key = customKeypad.getKey();

    if (key) {
      if (key >= '0' && key <= '9') {
        input += key;
      }
      if (key == 'A') {
        if (input.toInt() == 306) {

          menu = 3;  // pass benar
        } else {

          menu = 4;  // pass salah
        }
      }
      if (key == 'B') {
        input = "";
        display.showNumberDec(0, true);
        menu = 0;
      }
      if (key == 'C') {
        input = "";  //clear 7segment
        //input clear
        gambarclear();
        delay(1000);
        menu = 1;
      }
      if (key == 'D') {
        input.remove(input.length() - 1);
      }
      display.showNumberDec(input.toInt(), true);  //keypad to 7segment
    }
  }
  while (menu == 3) {
    gambarbenar();
    display.showNumberDec(0, true);
    input = "";
    digitalWrite(BUZZER, HIGH);
    blinkcorrect();
    delay(1000);
    digitalWrite(BUZZER, LOW);
    delay(1000);
    menu = 5;
  }
  while (menu == 4) {
    gambarwrong();
    blinkfalse();
    display.showNumberDec(0, true);
    input = "";
    delay(1000);
    menu = 1;
  }
  while (menu == 5) {

    gambarcolor();
    led(1, 1, 1);
    float red, green, blue;
    tcs.setInterrupt(false);
    delay(60);
    tcs.getRGB(&red, &green, &blue);
    tcs.setInterrupt(true);

    Serial.print("R:\t");
    Serial.print(int(red));
    Serial.print("\tG:\t");
    Serial.print(int(green));
    Serial.print("\tB:\t");
    Serial.print(int(blue));

    Serial.print("\n");

    if (red >= 170 && green >= 50 && blue >= 40)  //-> jika red
    {

      mendeteksi();
      gambarRed();

      stepper.moveTo(380);
      stepper.run();
    }

    else if (red >= 90 && green >= 110 && blue >= 35) {  //jika  yellow
      mendeteksi();
      gambarYellow();
    } else if (red >= 35 && green >= 105 && blue >= 110) {  //jika  blue
      mendeteksi();
      gambarBlue();
    } else if (red >= 45 && green >= 138 && blue >= 65) {  //jika  green
      mendeteksi();
      gambarGreen();
    } else if (red >= 90 && green >= 95 && blue >= 70) {  //jika  black
      mendeteksi();
      gambarBlack();
    } else if (red >= 85 && green >= 98 && blue >= 65) {  //jika  white
      mendeteksi();
      gambarWhite();
    }

    char key = customKeypad.getKey();
    if (key) {
      if (key == 'A') {
        menu = 0;
        input = "";
        i = 0;
      }
      if (key == 'B') {
        menu = 1;
        input = "";
      }
      if (key == 'C') {
        myStepper.setSpeed(10);
        myStepper.step(-339.6667);
      }
      if (key == 'D') {
        myStepper.setSpeed(10);
        myStepper.step(339.6667);
      }
    }
  }
}
