void mendeteksi() {
  // led(0, 0, 0);
  // digitalWrite(BUZZER, HIGH);
  // delay(200);
  // digitalWrite(BUZZER, LOW);
  // delay(200);
  // digitalWrite(BUZZER, HIGH);
  // delay(200);
  //digitalWrite(BUZZER, LOW);
}
void led(int r, int y, int g) {
  digitalWrite(RED, r);
  digitalWrite(YELLOW, y);
  digitalWrite(GREEN, g);
}

void kelip() {
  for (i; i <= 2; i++) {
    led(1, 1, 1);
    display.showNumberDec(0, true);
    delay(100);
    led(0, 0, 0);
    display.clear();
    delay(100);
  }
}
void blinkcorrect() {
  led(0, 0, 1);
  display.showNumberDec(0, true);
  delay(100);
  led(0, 0, 0);
  display.clear();
  delay(100);
  led(0, 0, 1);
  display.showNumberDec(0, true);
  delay(100);
  led(0, 0, 0);
  display.clear();
  delay(100);
}
void blinkfalse() {
  led(1, 0, 0);  //1
  display.showNumberDec(0, true);
  delay(100);
  led(0, 0, 0);
  display.clear();
  delay(100);
  led(1, 0, 0);  //2
  display.showNumberDec(0, true);
  digitalWrite(BUZZER, HIGH);
  delay(100);
  led(0, 0, 0);
  display.clear();
  digitalWrite(BUZZER, LOW);
  delay(100);
  led(1, 0, 0);  //1
  display.showNumberDec(0, true);
  delay(100);
  led(0, 0, 0);
  display.clear();
  delay(100);
  led(1, 0, 0);  //2
  display.showNumberDec(0, true);
  digitalWrite(BUZZER, HIGH);
  delay(100);
  led(0, 0, 0);
  display.clear();
  digitalWrite(BUZZER, LOW);
  delay(100);
}
void gambarhello() {
  display1.clearDisplay();
  // Display bitmap
  display1.drawBitmap(0, 0, hellohello, 128, 64, WHITE);
  display1.display();
}

void gambarRed() {
  display1.clearDisplay();
  // Display bitmap
  display1.drawBitmap(0, 0, i0_red, 128, 64, WHITE);
  display1.display();
  delay(2000);
}

void gambarYellow() {
  display1.clearDisplay();
  // Display bitmap
  display1.drawBitmap(0, 0, i0_Yellow, 128, 64, WHITE);
  display1.display();
  delay(2000);
}
